<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#BBEAFC">
    <include
        android:id="@+id/include"
        layout="@layout/toolbar" />
    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nunito_semibold"
        android:text="YOUR NAME: "
        android:layout_below="@+id/include"
        android:layout_margin="30dp"
        android:layout_alignParentStart="true"
        android:textColor="#000000"/>
    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nunito_semibold"
        android:text="CONTACT NO: "
        android:layout_below="@+id/textView11"
        android:layout_margin="30dp"
        android:layout_alignParentStart="true"
        android:textColor="#000000"/>
    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CITY NAME: "
        android:layout_below="@+id/textView12"
        android:layout_margin="30dp"
        android:layout_alignParentStart="true"
        android:textColor="#000000"/>
    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nunito_semibold"
        android:text="ADDRESS: "
        android:layout_below="@+id/textView13"
        android:layout_margin="30dp"
        android:layout_alignParentStart="true"
        android:textColor="#000000"/>
    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nunito_semibold"
        android:text="PIN CODE: "
        android:layout_below="@+id/textView14"
        android:layout_margin="30dp"
        android:layout_alignParentStart="true"
        android:textColor="#000000"/>
    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nunito_semibold"
        android:text="STATE: "
        android:layout_below="@+id/textView15"
        android:layout_margin="30dp"
        android:layout_alignParentStart="true"
        android:textColor="#000000"/>
    <EditText
        android:id="@+id/editTextPersonName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:fontFamily="@font/nunito_semibold"
        android:inputType="textPersonName"
        android:layout_below="@+id/include"
        android:layout_alignParentEnd="true"
        android:layout_margin="15dp"
        />
    <EditText
        android:id="@+id/editTextPhone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:fontFamily="@font/nunito_semibold"
        android:hint="##########"
        android:inputType="phone"
        android:text="+91"
        android:textColorHint="#000000"
        android:layout_below="@+id/editTextPersonName"
        android:layout_alignParentEnd="true"
        android:layout_margin="15dp" />
    <EditText
        android:id="@+id/editTextPersonName3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:ems="10"
        android:fontFamily="@font/nunito_semibold"
        android:inputType="textPersonName"
        android:layout_below="@+id/editTextPhone"
        android:layout_alignParentEnd="true"
        android:layout_margin="15dp" />
    <EditText
        android:id="@+id/editTextPostalAddress2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:fontFamily="@font/nunito_semibold"
        android:inputType="textPostalAddress"
        android:layout_below="@+id/editTextPersonName3"
        android:layout_alignParentEnd="true"
        android:layout_margin="15dp" />
    <EditText
        android:id="@+id/editTextPostalAddress3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:fontFamily="@font/nunito_semibold"
        android:hint="######"
        android:inputType="textPostalAddress"
        android:layout_below="@+id/editTextPostalAddress2"
        android:layout_alignParentEnd="true"
        android:layout_margin="15dp" />
    <EditText
        android:id="@+id/editTextPersonName7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editTextPostalAddress3"
        android:layout_alignParentEnd="true"
        android:ems="10"
        android:fontFamily="@font/nunito_semibold"
        android:inputType="textPersonName"
        android:layout_margin="30dp"/>
    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#2d7c8c"
        android:text="CANCEL"
        android:layout_below="@id/textView16"
        android:layout_margin="45dp"
        android:layout_alignParentStart="true"
        android:textColor="#FFFFFF"/>
    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#2d7c8c"
        android:text="NEXT"
        android:layout_below="@id/editTextPersonName7"
        android:layout_margin="45dp"
        android:layout_alignParentEnd="true"
        android:textColor="#FFFFFF"/>
</RelativeLayout>